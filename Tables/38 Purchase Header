    END;

    PROCEDURE SetStatus@53(NewStatus@1000 : Option);
    BEGIN
      Status := NewStatus;
      MODIFY;
    END;

    PROCEDURE TriggerOnAfterPostPurchaseDoc@116(VAR GenJnlPostLine@1001 : Codeunit 12;PurchRcpHdrNo@1002 : Code[20];RetShptHdrNo@1003 : Code[20];PurchInvHdrNo@1004 : Code[20];PurchCrMemoHdrNo@1005 : Code[20]);
    VAR
      PurchPost@1000 : Codeunit 90;
    BEGIN
      PurchPost.OnAfterPostPurchaseDoc(Rec,GenJnlPostLine,PurchRcpHdrNo,RetShptHdrNo,PurchInvHdrNo,PurchCrMemoHdrNo);
    END;

    PROCEDURE DeferralHeadersExist@38() : Boolean;
    VAR
      DeferralHeader@1000 : Record 1701;
      DeferralUtilities@1001 : Codeunit 1720;
    BEGIN
      DeferralHeader.SETRANGE("Deferral Doc. Type",DeferralUtilities.GetPurchDeferralDocType);
      DeferralHeader.SETRANGE("Gen. Jnl. Template Name",'');
      DeferralHeader.SETRANGE("Gen. Jnl. Batch Name",'');
      DeferralHeader.SETRANGE("Document Type","Document Type");
      DeferralHeader.SETRANGE("Document No.","No.");
      EXIT(NOT DeferralHeader.ISEMPTY);
    END;

    LOCAL PROCEDURE ConfirmUpdateDeferralDate@85();
    BEGIN
      IF HideValidationDialog THEN
        Confirmed := TRUE
      ELSE
        Confirmed := CONFIRM(DeferralLineQst,FALSE,FIELDCAPTION("Posting Date"));
      IF Confirmed THEN
        UpdatePurchLines(PurchLine.FIELDCAPTION("Deferral Code"),FALSE);
    END;

    LOCAL PROCEDURE "***TOPAS_Func***"@1000000000();
    BEGIN
    END;

    PROCEDURE CopyFromOrderLines@1140000(PurchDocNo@1140000 : Code[20];OrderDocNo@1140004 : Code[20]);
    VAR
      SalesLine@1140001 : Record 37;
      ArchSalesLine@1140002 : Record 5108;
      PurchLine@1140003 : Record 39;
      PurchLine2@1140006 : Record 39;
      NextLineNo@1140005 : Integer;
    BEGIN
      IF OrderDocNo = '' THEN EXIT;

      PurchLine.SETRANGE("Document Type",PurchLine."Document Type"::Order);
      PurchLine.SETRANGE("Document No.",PurchDocNo);
      IF PurchLine.FINDFIRST THEN
        ERROR('Die Bestellung ist nicht leer.');

      NextLineNo := 0;
      SalesLine.RESET;
      SalesLine.SETFILTER(Type,'%1',SalesLine.Type::Item);
      SalesLine.SETFILTER("Document No.",'%1',OrderDocNo);
      SalesLine.SETFILTER(Quantity,'>%1',0);
      IF SalesLine.FINDFIRST THEN
        REPEAT
          PurchLine.INIT;
          PurchLine."Document Type" := PurchLine."Document Type"::Order;
          PurchLine."Document No." := PurchDocNo;
          PurchLine."Line No." := NextLineNo + 10000;
          PurchLine.Type := PurchLine.Type :: Item;
          PurchLine.VALIDATE("No.",SalesLine."No.");
          PurchLine.VALIDATE(Quantity,SalesLine."Outstanding Quantity");
          PurchLine.INSERT(TRUE);
          PurchLine2.COPYFILTERS(PurchLine);
          IF PurchLine2.FINDLAST THEN
             NextLineNo := PurchLine2."Line No.";
        UNTIL SalesLine.NEXT = 0
      ELSE BEGIN
        ArchSalesLine.RESET;
        ArchSalesLine.SETFILTER(Type,'%1',ArchSalesLine.Type::Item);
        ArchSalesLine.SETFILTER("Document No.",'%1',OrderDocNo);
        ArchSalesLine.SETFILTER(Quantity,'>%1',0);
        IF ArchSalesLine.FINDFIRST THEN
          REPEAT
            PurchLine.INIT;
            PurchLine."Document Type" := PurchLine."Document Type"::Order;
            PurchLine."Document No." := PurchDocNo;
            PurchLine."Line No." := NextLineNo + 10000;
            PurchLine.Type := PurchLine.Type :: Item;
            PurchLine.VALIDATE("No.",ArchSalesLine."No.");
            PurchLine.VALIDATE(Quantity,ArchSalesLine.Quantity);
            PurchLine.INSERT(TRUE);
            PurchLine2.COPYFILTERS(PurchLine);
            IF PurchLine2.FINDLAST THEN
                NextLineNo := PurchLine2."Line No.";
          UNTIL ArchSalesLine.NEXT = 0;
      END;
    END;

    BEGIN
    {
      Lfd.Nr.  Version  Datum       User   Beschreibung
      ===========================================================================
      1        TOP1.00  20.10.16    UB     TOPAS Felder 50005,50006 und 50601-50604 angelegt
      2        TOP1.00  24.10.16    ST     TOPAS Programmierung Âbernommen
    }
    END.
  }
}

