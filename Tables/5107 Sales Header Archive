    END;

    PROCEDURE CreatToDoArch@1140000();
    VAR
      TempToDoArch@1140000 : TEMPORARY Record 5080;
    BEGIN
      TESTFIELD("Sell-to Contact No.");
      TempToDoArch.CreateToDoFromArchSalesHeader(Rec);
    END;

