                               PruefeSystemPos.RESET;
                               PruefeSystemPos.SETRANGE(Art,PruefeSystemPos.Art::System);
                               PruefeSystemPos.SETRANGE("Artikelnr.","Artikelnr.");
                               PruefeSystemPos.SETRANGE("Seriennr.","Seriennr.");
