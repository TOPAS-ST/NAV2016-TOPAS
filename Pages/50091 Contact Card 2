                 IF NOT MapMgt.TestSetup THEN
                   MapPointVisible := FALSE;

                 CRMIntegrationEnabled := CRMIntegrationManagement.IsCRMIntegrationEnabled;
               END;

    OnAfterGetRecord=BEGIN
                       //TOPAS 15.05.17 ST
                       IF (Type = Type::Company) AND (Rec."No." <> xRec."No.") AND ("No." <> '') THEN BEGIN
                         IF ("Betreuer Innendienst" = '') AND ("Salesperson Code" <> '')  THEN
                            MESSAGE('Der Betreuer Innendienst wurde noch nicht ausgew„hlt.');
                         IF ("Betreuer Innendienst" <> '') AND ("Salesperson Code" = '')  THEN
                            MESSAGE('Der Verk„ufercode wurde noch nicht ausgew„hlt.');
                         IF ("Betreuer Innendienst" = '') AND ("Salesperson Code" = '')  THEN
                            MESSAGE('Der Verk„ufercode und Betreuer Innendienst wurden noch nicht ausgew„hlt.');
                       END;

                       IF ("No." <> '') AND ("Company No." <> '') THEN BEGIN
                       ContactIndustryGroup.SETRANGE("Contact No.","Company No.");
                       ContactIndustryGroup.SETRANGE("Industry Group Code",'HŽNDLER');
                       IF ContactIndustryGroup.FINDFIRST THEN
                         Disti := TRUE
                       ELSE
                         Disti := FALSE;
                       END;

                       //29.08.08 ST - Anzeige "Angelegt von" aus den Žnderungsprotokollposten
                       ChangeLog.SETRANGE("Table No.",5050);
                       ChangeLog.SETRANGE("Type of Change",ChangeLog."Type of Change"::Insertion);
                       ChangeLog.SETRANGE("Primary Key Field 1 Value","No.");
                       IF ChangeLog.FINDFIRST THEN
                         CreatedBy := ChangeLog."User ID"
                       ELSE
                         CreatedBy := '';

                       //14.10.19 ST - Dynamisches Einblenden der Bemerkungen als gelbes Posted Feld
                       CommentFound := FALSE;
                       CommentText := '';

                       CommentLine.RESET;
                       CommentLine.SETRANGE("Table Name",CommentLine."Table Name"::Contact);
                       CommentLine.SETRANGE("No.","No.");
                       CommentLine.SETRANGE("Sub No.",0);
                       IF CommentLine.FINDFIRST THEN BEGIN
                           CommentFound := TRUE;
                         REPEAT
                           IF CommentText = '' THEN
                             CommentText += CommentLine.Comment
                           ELSE
                             IF CommentLine.Date <> 0D THEN
                               CommentText += ' --- ' + CommentLine.Comment
                             ELSE
                               CommentText += ' ' + CommentLine.Comment;
                         UNTIL CommentLine.NEXT = 0;
                       END;

                       IF (ControlReady) AND (CommentFound) THEN BEGIN
                         CurrPage.CommentControl.SendDatatoJavaScript(CommentText, 700, 82);
                       END;

                       //TOPAS Ende
                     END;

    OnNewRecord=VAR
                  Contact@1001 : Record 5050;
                BEGIN
                  IF GETFILTER("Company No.") <> '' THEN BEGIN
                    "Company No." := GETRANGEMAX("Company No.");
                    Type := Type::Person;
                    Contact.GET("Company No.");
                    InheritCompanyToPersonData(Contact,TRUE)
                  END;
                END;

