      ContactEditable@19051436 : Boolean INDATASET;
      SocialListeningSetupVisible@1000 : Boolean INDATASET;
      SocialListeningVisible@1003 : Boolean INDATASET;
      CRMIntegrationEnabled@1007 : Boolean;
      CRMIsCoupledToRecord@1006 : Boolean;
      OpenApprovalEntriesExistCurrUser@1008 : Boolean;
      OpenApprovalEntriesExist@1010 : Boolean;
      ShowWorkflowStatus@1011 : Boolean;
      "***TOPAS_Var***"@1000000000 : Integer;
      Customer@1000000008 : Record 18;
      ContactBusinessRelation@1000000002 : Record 5054;
      ContactIndustryGroup@1000000003 : Record 5058;
      ContComments@1000000011 : Record 5061;
      Wartungsvertrag@1000000007 : Record 50011;
      SalesInfoMgt@1000000009 : Codeunit 7171;
      Disti@1000000001 : Boolean;
      Text50000@1000000004 : TextConst 'DEU=Bitte hinterlegen Sie die Klassifizierung BESTšCKER im Kontakt.';
      Text50001@1000000005 : TextConst 'DEU=Es wurde die Kontaktklassifizierung BESTšCKER gefunden. Bitte kennzeichnen Sie auch den Debitor als Bestcker.';
      TextVersandbdgEnable@1000000006 : Boolean;
      Text50002@1000000010 : TextConst 'DEU=Der Debitor wurde noch nicht mit einem Kontakt verknpft.';
      CommentCont@1000000014 : Boolean;
      CommentText@1000000013 : Text;
      ControlReady@1000000012 : Boolean;

    LOCAL PROCEDURE ActivateFields@3();
    BEGIN
      SetSocialListeningFactboxVisibility;
      ContactEditable := "Primary Contact No." = '';
      //TOPAS +
      TextVersandbdgEnable := "besondere Versandbedingungen";
      //TOPAS -
    END;

