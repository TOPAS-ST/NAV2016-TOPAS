    OnAfterGetRecord=BEGIN
                       ActivateFields;
                       StyleTxt := SetStyle;

                       //TOPAS 22.07.08 ST +
                       Disti :=FALSE;
                       CommentCont := FALSE;
                       CommentText := '';

                       ContactBusinessRelation.RESET;
                       ContactBusinessRelation.SETCURRENTKEY("Link to Table","No.");
                       ContactBusinessRelation.SETRANGE("Link to Table",ContactBusinessRelation."Link to Table"::Customer);
                       ContactBusinessRelation.SETRANGE("No.","No.");
                       IF ContactBusinessRelation.FINDFIRST THEN BEGIN
                         //H„ndler Anzeige
                         ContactIndustryGroup.RESET;
                         ContactIndustryGroup.SETRANGE("Contact No.",ContactBusinessRelation."Contact No.");
                         ContactIndustryGroup.SETRANGE("Industry Group Code",'HŽNDLER');
                         IF ContactIndustryGroup.FINDFIRST THEN
                           Disti :=TRUE;
                         //Prft, ob Bemerkungen zum Kontakt existieren
                         ContComments.RESET;
                         ContComments.SETRANGE("Table Name",ContComments."Table Name"::Contact);
                         ContComments.SETRANGE("No.",ContactBusinessRelation."Contact No.");
                         IF ContComments.FINDFIRST THEN
                           REPEAT
                             CommentCont := TRUE;
                             IF CommentText = '' THEN
                               CommentText += FORMAT(ContComments.Date) + ' ' + ContComments.Comment
                             ELSE
                               IF ContComments.Date <> 0D THEN
                                 CommentText += ' --- ' + FORMAT(ContComments.Date) + ' ' + ContComments.Comment
                               ELSE
                                 CommentText += ' ' + ContComments.Comment;
                           UNTIL ContComments.NEXT = 0;

                         //Bestcker Hinweis
                         IF "No." <> xRec."No." THEN BEGIN
                           ContactIndustryGroup.RESET;
                           ContactIndustryGroup.SETFILTER("Contact No.",ContactBusinessRelation."Contact No.");
                           ContactIndustryGroup.SETFILTER("Industry Group Code",'BESTšCKER');
                           IF (NOT ContactIndustryGroup.FINDFIRST) AND ("Endkunde w/Bestcker" = TRUE) THEN
                             MESSAGE(Text50000)
                           ELSE
                           IF (ContactIndustryGroup.FINDFIRST) AND ("Endkunde w/Bestcker" = FALSE) THEN
                             MESSAGE(Text50001);
                         END;
                       END;


                       //TOPAS -
                     END;

