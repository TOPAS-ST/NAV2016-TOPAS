    PROCEDURE Export_Data_Excel@1140004();
    VAR
      ServicePos@1140000 : Record 50024;
      SalesShipHeader@1140004 : Record 110;
      Datens„tze_vorhanden@1140001 : Boolean;
      Datum_date@1140002 : Date;
      Datum_text@1140003 : Text[30];
      anzahl_daten@1140006 : Integer;
      Inhalt_A@1140005 : Text[30];
    BEGIN
      CLEAR(Ziel_Excel);
      CLEAR(Ziel_Book);
      CLEAR(Ziel_Sheet);

      anzahl_daten := 0;

      IF NOT FileManagement.ClientFileExists(Zielpfaddatei) THEN
        ERROR(Text014,Zielpfaddatei);

      ServicePos.RESET;
      ServicePos.SETFILTER(Art,'%1',Art::Service);
      ServicePos.SETFILTER("Zu exportieren",'%1',TRUE);
      ServicePos.SETFILTER(Exportiert,'%1',FALSE);
      IF ServicePos.FINDFIRST THEN BEGIN
        anzahl_daten := ServicePos.COUNT;

        Datum_date := WORKDATE();
        Datum_text := FORMAT(DATE2DMY(Datum_date,3)) + '-' + FORMAT(DATE2DMY(Datum_date,2)) + '-' + FORMAT(DATE2DMY(Datum_date,1));

        // Definiert die Zieldatei
        CREATE(Ziel_Excel,FALSE,TRUE);                                        //erzeugt excel
        Ziel_Book := Ziel_Excel.Workbooks._Open(Zielpfaddatei,0,FALSE);
        Ziel_Sheet := Ziel_Book.Worksheets.Item(1);                                  //definiert aktives worksheet
        Ziel_Excel.Visible(FALSE);
        Ziel_j := '1';

        //Syntax wird berprft
        Inhalt_A := Ziel_Sheet.Range('A1').Value;
        IF Inhalt_A <> 'Lfd. Nr.' THEN BEGIN
          ERROR('Der Export in Zieltabelle %1 wurde abgebrochen aufgrund fehlerhafter Syntax (Spalte A1 = %2 / '+
                'Richtig: Lfd. Nr.).',Zielpfaddatei,Inhalt_A);
        END;

        decstr := Ziel_Sheet.UsedRange.Rows.Count + 2;
        Ziel_j := FORMAT(decstr);
        Ziel_j_beginn := FORMAT(decstr);

        //Datens„tze werden nun in Excel eingefgt
        REPEAT

          //Suche nach 1. Basisger„t zum Andruck des Benuters + PW
          username := '';
          password := '';
          ZugangsdatenAndruck.RESET;
          ZugangsdatenAndruck.SETRANGE(Art,ZugangsdatenAndruck.Art::Service);
          ZugangsdatenAndruck.SETRANGE(Inaktiv,FALSE);
          ZugangsdatenAndruck.SETRANGE(Basisger„t,TRUE);
          ZugangsdatenAndruck.SETRANGE("Zu exportieren",TRUE);
          ZugangsdatenAndruck.SETRANGE("Wartungsvertragsnr.",ServicePos."Wartungsvertragsnr.");
          IF ZugangsdatenAndruck.FINDFIRST THEN BEGIN
            username := ZugangsdatenAndruck.Benutzername;
            password := ZugangsdatenAndruck.Passwort;
          END ELSE BEGIN
            //Wartungsvertrag verfgt ber kein Basisger„t
            ZugangsdatenAndruck.RESET;
            ZugangsdatenAndruck.SETRANGE(Art,ZugangsdatenAndruck.Art::Service);
            ZugangsdatenAndruck.SETRANGE(Inaktiv,FALSE);
            ZugangsdatenAndruck.SETRANGE("Zu exportieren",TRUE);
            ZugangsdatenAndruck.SETRANGE("Wartungsvertragsnr.",ServicePos."Wartungsvertragsnr.");
            IF ZugangsdatenAndruck.FINDFIRST THEN BEGIN
              username := ZugangsdatenAndruck.Benutzername;
              password := ZugangsdatenAndruck.Passwort;
            END
          END;

          Ziel_Sheet.Range('A'+Ziel_j).Value := ServicePos."Lfd. Nr.";
          Ziel_Sheet.Range('B'+Ziel_j).Value := TOPAS_API.CreateExcelHyperlinkToNav(Wartungsvertrag.TABLENAME,1,50015,
                                                ServicePos."Wartungsvertragsnr.");
          Ziel_Sheet.Range('C'+Ziel_j).Value := ServicePos."Artikelnr. 2";
          Ziel_Sheet.Range('D'+Ziel_j).Value := ServicePos."Seriennr.";
          Ziel_Sheet.Range('E'+Ziel_j).Value := ServicePos."SUP Artikelnr.";
          Ziel_Sheet.Range('F'+Ziel_j).Value := ServicePos."Auftragsnr.";
          Ziel_Sheet.Range('G'+Ziel_j).Value := TOPAS_API.CreateExcelHyperlinkToNav(SalesShipHeader.TABLENAME,1,130,
                                                ServicePos."Nr.");
          Ziel_Sheet.Range('H'+Ziel_j).Value := ServicePos.Belegdatum;
          Ziel_Sheet.Range('I'+Ziel_j).Value := ServicePos."Debitornr.";
          Ziel_Sheet.Range('J'+Ziel_j).Value := ServicePos.Debitorname;
          Ziel_Sheet.Range('K'+Ziel_j).Value := ServicePos."System Projektinfos";
          Ziel_Sheet.Range('L'+Ziel_j).Value := username;
          Ziel_Sheet.Range('M'+Ziel_j).Value := password;
          Ziel_Sheet.Range('N'+Ziel_j).Value := ServicePos.Lizenzdatum;
          Ziel_Sheet.Range('O'+Ziel_j).Value := ServicePos."Ablaufdatum Serviceleistung";
          Ziel_Sheet.Range('A'+Ziel_j + ':' + 'O' + Ziel_j).Font.Bold := TRUE;
          Ziel_j := INCSTR(Ziel_j);
          ServicePos.Exportiert := TRUE;
          ServicePos.Exportdatum := WORKDATE;
          ServicePos.MODIFY;
        UNTIL ServicePos.NEXT = 0;
      END;

      //šberprfung Export + Navision Datens„tze werden so gekennzeichnet, das kein Export mehr n”tig ist
      IF anzahl_daten > 0 THEN BEGIN
        Ziel_Book.Save;
        Ziel_Excel.Visible(TRUE);
        CLEAR(Ziel_Book);
        CLEAR(Ziel_Sheet);
      END ELSE BEGIN
        IF CONFIRM(Text016,TRUE) THEN BEGIN
          CREATE(Ziel_Excel,FALSE,TRUE);                                        //erzeugt excel
          Ziel_Book := Ziel_Excel.Workbooks._Open(Zielpfaddatei,0,FALSE);
          Ziel_Sheet := Ziel_Book.ActiveSheet;                                  //definiert aktives worksheet
          Ziel_Excel.Visible(TRUE);
          CLEAR(Ziel_Book);
          CLEAR(Ziel_Sheet);
        END;
      END;
    END;

    LOCAL PROCEDURE SetStyle@1000000004(expr@1000000000 : Text) : Text;
    BEGIN
      CASE expr OF

        'hinweis' :
          IF hinweis <> '' THEN
            EXIT('Unfavorable')
          ELSE
            EXIT('');

        'preisinfo' :
          IF preisinfo <> '' THEN
            EXIT('StrongAccent')
          ELSE
            EXIT('');

        'ordner' :
          IF ordner_vorhanden = 'Ja' THEN
            EXIT('StrongAccent')
          ELSE
            EXIT('');

      END;
    END;

