    OnAfterGetRecord=VAR
                       sup_artikelnr@1140000 : Code[20];
                     BEGIN
                       IF ("Service Artikelnr." <> '') AND (Art = Art :: Service) AND (("BET-Nr." = '') OR ("BET-Pos." = 0)) THEN
                         hinweis := 'BET offen'
                       ELSE
                         hinweis := '';

                       hinweis_style := SetStyle('hinweis');

                       //03.07.15 ST - Indikator dafr, wenn der Preis nicht aktuell ist
                       preisinfo := '';
                       sup_artikelnr := LizenzTab.FakeSUPArtikelErmitteln("SUP Artikelnr.");
                       IF ServicePreise.GET("Service Artikelnr.",sup_artikelnr,"Artikelnr. 2","Service W„hrungscode") THEN
                         IF ServicePreise."Update letzte Preisliste" = FALSE THEN
