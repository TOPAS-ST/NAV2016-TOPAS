                       aur_nr := '';
                       SalesShipmentLine.RESET;
                       SalesShipmentLine.SETRANGE("Document No.","Nr.");
                       SalesShipmentLine.SETRANGE("Line No.","Belegzeilennr.");
                       IF SalesShipmentLine.FINDFIRST THEN
                         aur_nr := SalesShipmentLine."Blanket Order No.";

                       //07.11.16 ST - Gibt die Laufzeit in Jahre und Tage aus
                       laufzeit_text := TOPAS_API.RuntimeText_output(Lizenzdatum,"Ablaufdatum Serviceleistung");

                       //11.01.18 ST - Gibt Hinweis auf Auftrags Bemerkung aus
                       SalesCommentLine.SETRANGE("Document Type",SalesCommentLine."Document Type"::Order);
                       SalesCommentLine.SETRANGE("No.","Auftragsnr.");
                       SalesCommentLine.SETRANGE("Document Line No.",0);
                       IF SalesCommentLine.FINDFIRST THEN
                         bemerkung_au := 'Ja'
                       ELSE BEGIN
                         SalesCommentLineArchive.SETRANGE("Document Type",SalesCommentLineArchive."Document Type"::Order);
                         SalesCommentLineArchive.SETRANGE("No.","Auftragsnr.");
                         SalesCommentLineArchive.SETRANGE("Document Line No.",0);
                         IF SalesCommentLineArchive.FINDFIRST THEN
                           bemerkung_au := 'Ja'
                         ELSE
                           bemerkung_au := 'Nein';
                       END;

                       //09.02.18 ST - Gibt an, ob ein Ordner zum Wartungsvertrag existiert
                       IF Wartungsvertrag.GET("Wartungsvertragsnr.") THEN
                         IF Wartungsvertrag."Ordner vorhanden" THEN
                           ordner_vorhanden := 'Ja'
                         ELSE
                           ordner_vorhanden := 'Nein';

                       ordner_vorhanden_style := SetStyle('ordner');
                     END;

    OnNewRecord=BEGIN
                  IF (Art = Art :: Service) AND (GETFILTER("Debitornr.") <> '') AND ("Debitornr." = '') THEN
                    "Debitornr." := GETFILTER("Debitornr.");
                END;

