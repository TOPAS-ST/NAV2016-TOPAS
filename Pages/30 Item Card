OBJECT Page 30 Item Card
{
  OBJECT-PROPERTIES
  {
    Date=30.04.19;
    Time=15:17:21;
    Modified=Yes;
    Version List=NAVW19.00.00.50528,NAVDACH9.00.00.50528,TOP1.03;
  }
  PROPERTIES
  {
    CaptionML=[DEU=Artikelkarte;
               ENU=Item Card];
    SourceTable=Table27;
    PageType=Card;
    RefreshOnActivate=Yes;
    PromotedActionCategoriesML=[DEU=Neu,Verarbeiten,Bericht,Genehmigen,Infos;
                                ENU=New,Process,Report,Approve,Infos];
    OnInit=BEGIN
             UnitCostEnable := TRUE;
             StandardCostEnable := TRUE;
             OverflowLevelEnable := TRUE;
             DampenerQtyEnable := TRUE;
             DampenerPeriodEnable := TRUE;
             LotAccumulationPeriodEnable := TRUE;
             ReschedulingPeriodEnable := TRUE;
             IncludeInventoryEnable := TRUE;
             OrderMultipleEnable := TRUE;
             MaximumOrderQtyEnable := TRUE;
             MinimumOrderQtyEnable := TRUE;
             MaximumInventoryEnable := TRUE;
             ReorderQtyEnable := TRUE;
             ReorderPointEnable := TRUE;
             SafetyStockQtyEnable := TRUE;
             SafetyLeadTimeEnable := TRUE;
             TimeBucketEnable := TRUE;
           END;

    OnOpenPage=VAR
                 CRMIntegrationManagement@1000 : Codeunit 5330;
               BEGIN
                 EnableShowStockOutWarning;
                 EnableShowShowEnforcePositivInventory;
                 CRMIntegrationEnabled := CRMIntegrationManagement.IsCRMIntegrationEnabled;
               END;

    OnAfterGetRecord=BEGIN
                       EnablePlanningControls;
                       EnableCostingControls;
                       SetSocialListeningFactboxVisibility;

                       //TOPAS 03.03.17 ST +
                       Anzahl_RMA := 0;
                       RMA_Artikel.RESET;
                       RMA_Artikel.SETRANGE(Art,RMA_Artikel.Art::Artikel);
                       RMA_Artikel.SETRANGE("Artikelnr.","No.");
                       IF RMA_Artikel.FINDFIRST THEN
                         Anzahl_RMA := RMA_Artikel.COUNT;

                       //Berechnung der Rahmenrestmenge
                       Rahmenrestmenge := 0;
                       SalesLine.SETRANGE(Type,SalesLine.Type::Item);
                       SalesLine.SETRANGE("Document Type",SalesLine."Document Type"::"Blanket Order");
                       SalesLine.SETRANGE("No.","No.");
                       IF SalesLine.FINDFIRST THEN
                         REPEAT
                           SalesLine.CALCFIELDS("offene Menge aus Rahmen im AU");
                           Rahmenrestmenge := Rahmenrestmenge + (SalesLine."Quantity (Base)" - SalesLine."Qty. Shipped (Base)" -
                                              SalesLine."offene Menge aus Rahmen im AU");
                         UNTIL SalesLine.NEXT=0;

                       //Lieferzeit wird berechnet
                       Lieferzeiten_Text := '';

                       Lieferzeit.RESET;
                       Lieferzeit.SETRANGE(Art,Lieferzeit.Art::Artikel);
                       Lieferzeit.SETRANGE("Nr.","No.");
                       IF (Lieferzeit.FINDFIRST) AND (Lieferzeit.Bemerkung <> '') THEN
                         Lieferzeiten_Text := Lieferzeit.Bemerkung + ' (Fr Artikel)'
                       ELSE BEGIN
                         Lieferzeit.RESET;
                         Lieferzeit.SETRANGE(Art,Lieferzeit.Art::Produktgruppe);
                         Lieferzeit.SETRANGE("Hersteller der Produktgruppe","Manufacturer Code");
                         Lieferzeit.SETRANGE("Nr.",Artikelgruppe);
                         IF(Lieferzeit.FINDFIRST) AND (Lieferzeit.Bemerkung <> '') THEN
                           Lieferzeiten_Text := Lieferzeit.Bemerkung + ' ('+Artikelgruppe+')'
                         ELSE BEGIN
                           Lieferzeit.RESET;
                           Lieferzeit.SETRANGE(Art,Lieferzeit.Art::Hersteller);
                           Lieferzeit.SETRANGE("Nr.","Manufacturer Code");
                           IF (Lieferzeit.FINDFIRST) AND (Lieferzeit.Bemerkung <> '') THEN
                             Lieferzeiten_Text := Lieferzeit.Bemerkung + ' (Fr Hersteller)';
                         END;
                       END;

                       //Anzeige des DC-Preises und Einf„rbung nach entsprechendem W„hrungscode des Kreditors
                       "v_DC-Preis" := TOPAS_Funktionen.FindItemPurchPrice("No.","Vendor No.",WORKDATE,"Base Unit of Measure");
                       IF Vendor.GET("Vendor No.") THEN
                         dc_style := TOPAS_Funktionen.SetCurrencyColor(Vendor."Currency Code")
                       ELSE
                         dc_style := 'Standard';

                       //Anzeige der VK-Preise
                       "v_VK-Preis_Euro" := TOPAS_Funktionen.FindItemSalesPrice("No.",WORKDATE,'',"Base Unit of Measure",1,0);
                       "v_VK-Preis_USD" := TOPAS_Funktionen.FindItemSalesPrice("No.",WORKDATE,'USD',"Base Unit of Measure",1,0);

                       //Visualisierung der Verfgbarkeit
                       Verfuegbarkeit := Inventory + "Qty. on Purch. Order" - "Qty. on Sales Order"-"Menge in Leihstellung";
                       IF Verfuegbarkeit = 0 THEN
                         verfuegbarkeit_style := 'Standard'
                       ELSE
                         IF Verfuegbarkeit > 0 THEN
                           verfuegbarkeit_style := 'Favorable'
                         ELSE
                           IF Verfuegbarkeit < 0 THEN
                             verfuegbarkeit_style := 'Unfavorable';

                       //04.07.17 ST ST - Hebt das Bemerkungs Feld rot hervor, wenn eine Bemerkung zum Artikel vorhanden ist
                       IF Comment THEN
                         comment_style := 'Unfavorable'
                       ELSE
                         comment_style := 'Standard';

                       //TOPAS -
                     END;

    OnNewRecord=BEGIN
                  EnablePlanningControls;
                  EnableCostingControls;

                  //TOPAS 30.03.2017 ST +
                  "v_DC-Preis" := 0;
                  //TOPAS -
                END;

    OnAfterGetCurrRecord=VAR
                           CRMCouplingManagement@1001 : Codeunit 5331;
