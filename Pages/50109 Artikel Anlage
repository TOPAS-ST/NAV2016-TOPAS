      Quelle_Excel@1000000052 : Automation "{00020813-0000-0000-C000-000000000046} 1.2:{00024500-0000-0000-C000-000000000046}:Unknown Automation Server.Application";
      Quelle_Book@1000000051 : Automation "{00020813-0000-0000-C000-000000000046} 1.2:{00020819-0000-0000-C000-000000000046}:Unknown Automation Server.Workbook";
      Quelle_Sheet@1000000050 : Automation "{00020813-0000-0000-C000-000000000046} 1.2:{00020820-0000-0000-C000-000000000046}:Unknown Automation Server.Worksheet";
      Quelle_j@1000000049 : Text[7];
      PL_Excel@1000000048 : Automation "{00020813-0000-0000-C000-000000000046} 1.7:{00024500-0000-0000-C000-000000000046}:'Microsoft Excel 14.0 Object Library'.Application";
      PL_Book@1000000047 : Automation "{00020813-0000-0000-C000-000000000046} 1.7:{00020819-0000-0000-C000-000000000046}:'Microsoft Excel 14.0 Object Library'.Workbook";
      PL_Sheet@1000000046 : Automation "{00020813-0000-0000-C000-000000000046} 1.7:{00020820-0000-0000-C000-000000000046}:'Microsoft Excel 14.0 Object Library'.Worksheet";
      v_PL_EURO_Pfad@1000000045 : Text[250];
      v_PL_USD_Pfad@1000000044 : Text[250];
      v_pricebook_sheetname_EURO@1000000043 : Text[50];
      v_pricebook_sheetname_USD@1000000042 : Text[50];
      v_Spalte_PL_Part@1000000041 : Text[10];
      v_Spalte_PL_MSRP@1000000040 : Text[10];
      v_Spalte_PL_LongDescr@1000000039 : Text[10];
      v_Spalte_PL_ShortDescr@1000000038 : Text[10];
      cell_part@1000000037 : Text[30];
      cell_services@1000000036 : Text[30];
      cell_training@1000000035 : Text[30];
      row_part@1000000034 : Integer;
      row_services@1000000033 : Integer;
