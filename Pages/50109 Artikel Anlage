                GroupType=Group }

    { 1000000002;3;Field  ;
                CaptionML=DEU=Datei;
                SourceExpr=filename;
                OnLookup=BEGIN
                           TOPAS_API.OpenFile(filename);
                         END;

                OnAssistEdit=BEGIN
                               filename := TOPAS_API.Save_FileDialog;
                             END;
                              }

  }
  CODE
  {
    VAR
      einkaufspreis@1000000058 : Record 7012;
      verkaufspreis@1000000057 : Record 7002;
