               OnAfterGetRecord=BEGIN
                                  Sheet.Range('A'+j).Value := TOPAS_API.CreateExcelHyperlinkToNav(Wartungsvertrag.TABLENAME,1,50015,
                                                              Wartungsvertrag."Wartungsvertragsnr.");
