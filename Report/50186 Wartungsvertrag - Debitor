                                      Sheet.Range('S'+j).Value := benutzer;
                                      Sheet.Range('T'+j).Value := pw;
                                    END;
                                    j := INCSTR(j);
                                  END;
                                END;

               DataItemLink=Wartungsvertragsnr.=FIELD(Wartungsvertragsnr.) }

