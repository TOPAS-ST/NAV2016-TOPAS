    { 31  ;    ;DataItem;CopyHeader          ;
               DataItemTable=Table2000000026;
               DataItemTableView=SORTING(Number);
               OnPreDataItem=BEGIN
                               Berichtstabelle_kopf.RESET;
                               Berichtstabelle_kopf.SETFILTER(Tabelle,'%1|%2','MEMOKOPF','AUFGABE');
                               Berichtstabelle_kopf.SETCURRENTKEY(Datum,Index);
                               Berichtstabelle_kopf.ASCENDING(FALSE);
                               SETRANGE(Number,1,Berichtstabelle_kopf.COUNT);
                             END;

               OnAfterGetRecord=BEGIN
                                  IF Number = 1 THEN
                                    Berichtstabelle_kopf.FIND('-')
                                  ELSE
                                    Berichtstabelle_kopf.NEXT;

                                  vLinientext := '';
                                  vAnsprechpartner := '';
                                  text1 := '';
                                  text2 := '';

                                  IF Berichtstabelle_kopf.Tabelle = 'MEMOKOPF' THEN BEGIN
                                    Memokopf_Druck.SETRANGE("Memonr.",Berichtstabelle_kopf.Index);
                                    IF Memokopf_Druck.FINDFIRST THEN
                                      beschreibung := Memokopf_Druck.Memokurztext;

                                    text1 := 'Erfasser: ';
                                    text2 := 'Memodatum: ';

                                    Linien.RESET;
                                    Linien.SETFILTER("Memonr.",Berichtstabelle_kopf.Index);
                                    IF Linien.FINDFIRST THEN BEGIN
                                      REPEAT
                                        IF Hersteller.GET(Linien.Linienk¬Årzel) THEN BEGIN
                                          IF vLinientext = '' THEN
                                            vLinientext := Hersteller.Herstellerkurzname
                                          ELSE
                                            vLinientext := vLinientext + ' ' + Hersteller.Herstellerkurzname;
                                        END;
                                      UNTIL Linien.NEXT = 0;
                                    END;

                                    IF vLinientext = '' THEN
                                      vLinientext := Berichtstabelle_kopf."Feld 7";

                                    Ansprech.RESET;
                                    Ansprech.SETFILTER(Typ,'%1',Ansprech.Typ::Person);
                                    Ansprech.SETFILTER("Memonr.",'%1',Berichtstabelle_kopf.Index);
                                    IF Ansprech.FINDFIRST THEN BEGIN
                                      REPEAT
                                        Ansprech.CALCFIELDS(Kontaktname);
                                        IF vAnsprechpartner = '' THEN
                                          vAnsprechpartner := Ansprech.Kontaktname
                                        ELSE
                                          IF STRLEN(vAnsprechpartner + Ansprech.Kontaktname + '...weitere s. Memo') < 80 THEN
                                            vAnsprechpartner := vAnsprechpartner + ', ' + Ansprech.Kontaktname
                                          ELSE
                                            IF STRPOS(vAnsprechpartner,'...weitere s. Memo') = 0 THEN
                                              vAnsprechpartner := vAnsprechpartner + '...weitere s. Memo';
                                        UNTIL Ansprech.NEXT = 0;
                                    END;
                                    IF vAnsprechpartner = '' THEN
                                      vAnsprechpartner := Berichtstabelle_kopf."Feld 10" + '         ' +  Berichtstabelle_kopf."Feld 11";
                                  END;

                                  IF Berichtstabelle_kopf.Tabelle = 'AUFGABE' THEN BEGIN
                                    Aufgabe_Druck.SETRANGE("No.",Berichtstabelle_kopf.Index);
                                    IF Aufgabe_Druck.FINDFIRST THEN
                                      beschreibung := Aufgabe_Druck.Description;
                                    text1 := 'Bearbeiter: ';
                                    text2 := 'N‚Äûchstes Follow Up: ';
                                    vAnsprechpartner := Berichtstabelle_kopf."Feld 10" + '         ' +  Berichtstabelle_kopf."Feld 11";
                                    vLinientext := Berichtstabelle_kopf."Feld 7";
                                  END;
                                END;
                                 }

