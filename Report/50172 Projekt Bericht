    { 1000000006;1;DataItem;CopyLine         ;
               DataItemTable=Table2000000026;
               DataItemTableView=WHERE(Number=FILTER(>0));
               OnPreDataItem=BEGIN
                               Berichtstabelle_zeilen.RESET;
                               Berichtstabelle_zeilen.SETCURRENTKEY(Tabelle,Index,"Belegnr.","Zeilennr.");
                               Berichtstabelle_zeilen.SETFILTER(Tabelle,'%1|%2','BEMERKUNG','MEMOZEILE');
                               Berichtstabelle_zeilen.SETRANGE(Index,Berichtstabelle_kopf.Index);
