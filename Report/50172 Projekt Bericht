                                  IF Projekt.GET(Campaign."No.") THEN BEGIN
                                    Berichtstabelle.INIT;
                                    Berichtstabelle.Tabelle := 'Projekt';
                                    Berichtstabelle.Index := Projekt."No.";
                                    Berichtstabelle."Feld 1" := Projekt.Description;
                                    Berichtstabelle."Feld 2" := Projekt.Projektstatus;
                                    Berichtstabelle."Feld 3" := Projekt.Projektklasse;
                                    Berichtstabelle."Feld 4" := Projekt.Projektvolumen;
                                    Berichtstabelle."Feld 5" := Projekt.Projektwert;
                                    Berichtstabelle."Feld 6" := Projekt."Salesperson Code";
                                    Berichtstabelle."Feld 7" := Projekt."Betreuer Innendienst";
                                    Berichtstabelle."Feld 8" := Projekt.Teams;
                                    Berichtstabelle."Feld 9"  := Projekt."Territory Code";
                                    Berichtstabelle."Feld 10" := Projekt.Applikation;
                                    Berichtstabelle."Feld 11" := Projekt.Wettbewerb;
                                    Berichtstabelle."Feld 12" := Projekt.Prototyping;
                                    Berichtstabelle."Feld 13" := Projekt.Vorserienstart;
                                    Berichtstabelle."Feld 14" := Projekt."Serienstart (geplant)";
                                    Berichtstabelle."Feld 15" := Projekt."Aktueller Serienstart";
                                    Berichtstabelle."Feld 16" := Projekt."Hersteller Projektnr.";
                                    Berichtstabelle."Feld 19" := FORMAT(Projekt."Aktueller Stand");
                                    Berichtstabelle.Uhrzeit := CURRENTDATETIME;
                                    Berichtstabelle.User := USERID;
                                    Berichtstabelle.INSERT;
                                  END;
                                  Ansprech.RESET;
                                  Ansprech.SETFILTER("Kampagnennr.",'%1',"No.");
                                  IF Ansprech.FINDFIRST THEN BEGIN
                                    REPEAT
                                      Ansprech.CALCFIELDS(Kontaktname,Mandantenname,"Telefonnr.");
                                      Berichtstabelle.INIT;
                                      Berichtstabelle.Tabelle := 'Ansprechpartner';
                                      Berichtstabelle.Index := Ansprech."Kontaktnr.";
                                      Berichtstabelle."Belegnr." := Ansprech."Kampagnennr.";
                                      Berichtstabelle."Feld 1" := Ansprech.Bemerkung;
                                      Berichtstabelle."Feld 2" := Ansprech."Kontakt Mandantennr.";
                                      Berichtstabelle."Feld 10" := Ansprech.Mandantenname;
                                      Berichtstabelle."Feld 11" := Ansprech.Kontaktname;
                                      Berichtstabelle."Feld 12" := Ansprech."Telefonnr.";
                                      Berichtstabelle."Feld 17" := Ansprech.Best¬Åcker;
                                      Berichtstabelle.Uhrzeit := CURRENTDATETIME;
                                      Berichtstabelle.User := USERID;
                                      Berichtstabelle.INSERT;
                                    UNTIL Ansprech.NEXT = 0;
                                  END;
                                  Projartikel.RESET;
                                  Projartikel.SETRANGE("Kampagnennr.","No.");
                                  Projartikel.SETRANGE(Inaktiv,FALSE);
                                  IF Projartikel.FINDFIRST THEN BEGIN
                                    REPEAT
                                      Berichtstabelle.INIT;
                                      Berichtstabelle.Tabelle := 'Projektartikel';
                                      Berichtstabelle.Index := Projartikel."Kampagnennr.";
                                      Berichtstabelle."Zeilennr." := Projartikel."Zeilennr.";
                                      Berichtstabelle."Feld 2" := Projartikel."Artikelnr.";
                                      Berichtstabelle."Feld 3" := Projartikel."Artikelnr. 2";
                                      Berichtstabelle."Feld 4" := Projartikel."VK-Preis";
                                      Berichtstabelle."Feld 5" := Projartikel."Menge pro System";
                                      Berichtstabelle.Uhrzeit := CURRENTDATETIME;
                                      Berichtstabelle.User := USERID;
                                      Berichtstabelle.INSERT;
                                    UNTIL Projartikel.NEXT = 0;
                                  END;
                                  Memokopf.RESET;
                                  Memokopf.SETRANGE("Kampagnennr.","No.");
                                  IF Memokopf.FINDFIRST THEN BEGIN
                                    REPEAT
                                      Berichtstabelle.INIT;
                                      Berichtstabelle.Tabelle := 'Memokopf';
                                      Berichtstabelle.Index := Memokopf."Memonr.";
                                      Berichtstabelle.Datum := Memokopf.Anlagedatum;
                                      Berichtstabelle."Belegnr." := Memokopf."Kampagnennr.";
                                      Berichtstabelle."Feld 1" := Memokopf.Memokurztext;
                                      Berichtstabelle."Feld 3" := Memokopf.Kontaktart;
                                      Berichtstabelle."Feld 6" := Memokopf."Angelegt von";
                                      Berichtstabelle."Feld 18" := Memokopf.Memodatum;
                                      Berichtstabelle.Uhrzeit := CURRENTDATETIME;
                                      Berichtstabelle.User := USERID;
                                      Berichtstabelle.INSERT;
                                      Memozeile.RESET;
                                      Memozeile.SETRANGE("Memonr.",Memokopf."Memonr.");
                                      IF Memozeile.FINDFIRST THEN BEGIN
                                        REPEAT
                                          Berichtstabelle.INIT;
                                          Berichtstabelle.Tabelle := 'Memozeile';
                                          Berichtstabelle.Index := Memozeile."Memonr.";
                                          Berichtstabelle.Datum := Memozeile.Datum;
                                          Berichtstabelle."Belegnr." := Memozeile."Memonr.";
                                          Berichtstabelle."Zeilennr." := Memozeile."Zeilennr.";
                                          Berichtstabelle."Feld 1" := Memozeile.Bemerkung;
                                          Berichtstabelle.Uhrzeit := CURRENTDATETIME;
                                          Berichtstabelle.User := USERID;
                                          Berichtstabelle.INSERT;
                                        UNTIL Memozeile.NEXT = 0;
                                      END;
                                    UNTIL Memokopf.NEXT = 0;
                                  END;
                                  Aufgabe.RESET;
                                  Aufgabe.SETRANGE("System To-do Type",Aufgabe."System To-do Type"::Organizer);
                                  Aufgabe.SETRANGE("Campaign No.","No.");
                                  IF Aufgabe.FINDFIRST THEN BEGIN
                                    REPEAT
                                      Aufgabe.CALCFIELDS("Contact Name","Contact Company Name");
                                      Berichtstabelle.INIT;
                                      Berichtstabelle.Tabelle := 'Aufgabe';
                                      Berichtstabelle.Index := Aufgabe."No.";
                                      IF Aufgabe."Erstellt am" <> 0D THEN
                                        Berichtstabelle.Datum := Aufgabe."Erstellt am"
                                      ELSE
                                        Berichtstabelle.Datum := Aufgabe.Date;
                                      Berichtstabelle."Belegnr." := Aufgabe."No.";
                                      Berichtstabelle."Feld 1" := Aufgabe.Description;
                                      IF FORMAT(Aufgabe.Type) = ' ' THEN
                                        Berichtstabelle."Feld 3" := 'AUFGABE'
                                      ELSE
                                        Berichtstabelle."Feld 3" := FORMAT(Aufgabe.Type);
                                      Berichtstabelle."Feld 6" := Aufgabe."Salesperson Code";
                                      Hersteller.RESET;
                                      IF Hersteller.GET(Aufgabe.Linie) THEN
                                      Berichtstabelle."Feld 7" := Hersteller.Herstellerkurzname;
                                      Berichtstabelle."Feld 10" := Aufgabe."Contact Name";
                                      Berichtstabelle."Feld 11" := Aufgabe."Contact Company Name";
                                      Berichtstabelle."Feld 18" := Aufgabe.Date;
                                      Berichtstabelle.Uhrzeit := CURRENTDATETIME;
                                      Berichtstabelle.User := USERID;
                                      Berichtstabelle.INSERT;
                                      Bemerkung.RESET;
                                      Bemerkung.SETRANGE("Table Name",Bemerkung."Table Name"::"To-do");
                                      Bemerkung.SETRANGE("No.",Aufgabe."No.");
                                      IF Bemerkung.FINDFIRST THEN BEGIN
                                        REPEAT
                                          Berichtstabelle.INIT;
                                          Berichtstabelle.Tabelle := 'Bemerkung';
                                          Berichtstabelle.Index := Bemerkung."No.";
                                          Berichtstabelle."Belegnr." := Bemerkung."No.";
                                          Berichtstabelle."Zeilennr." := Bemerkung."Line No.";
