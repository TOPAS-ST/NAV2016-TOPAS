               OnAfterGetRecord=BEGIN
                                  TariffNo := '';
                                  Artikelnr := '';
                                  Artikel.SETFILTER("No.",'%1',"Artikelnr.");
                                  IF Artikel.FINDFIRST THEN
                                    BEGIN
                                      TariffNo := Artikel."Tariff No.";
                                      Artikelnr := Artikel."No. 2";
