                               "Sales Invoice Header".SETFILTER("Sales Invoice Header"."Betreuer Innendienst",Betreuer);
                               "Sales Invoice Header".SETFILTER("Sales Invoice Header"."Salesperson Code",Verkcode);
                               "Sales Invoice Header".SETFILTER("Sales Invoice Header"."Territory Code",Gebcode);
                               IF (PstDateFrom <> 0D) AND (PstDateTo = 0D) THEN
                                 "Sales Invoice Header".SETFILTER("Sales Invoice Header"."Posting Date",'>=%1',PstDateFrom);
                               IF (PstDateFrom = 0D) AND (PstDateTo <> 0D) THEN
                                 "Sales Invoice Header".SETFILTER("Sales Invoice Header"."Posting Date",'<=%1',PstDateTo);
