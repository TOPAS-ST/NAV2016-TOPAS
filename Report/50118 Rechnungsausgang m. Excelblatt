    { 4   ;3   ;DataItem;                    ;
               DataItemTable=Table27;
               DataItemTableView=SORTING(No.);
               OnPreDataItem=BEGIN
                               Item.SETFILTER(Item."No.",Artikelnr);
                             END;

               OnAfterGetRecord=BEGIN
                                  IF ("Sales Invoice Line".Quantity>0) THEN
                                   BEGIN
                                     Sheet.Range('A'+j).Value := Customer."No.";
                                     Sheet.Range('B'+j).Value := Customer.Name;
                                     Sheet.Range('C'+j).Value := Customer."Name 2";
                                     Sheet.Range('D'+j).Value := Customer.Address;
                                     Sheet.Range('E'+j).Value := Customer."Address 2";
                                     Sheet.Range('F'+j).Value := Customer."Post Code";
                                     Sheet.Range('G'+j).Value := Customer.City;
                                     Sheet.Range('H'+j).Value := "Sales Invoice Line"."No.";
                                     Sheet.Range('I'+j).Value := FORMAT("Sales Invoice Line".Quantity);
                                     Sheet.Range('J'+j).Value := FORMAT("Sales Invoice Line"."VK-Preis (MW)");
                                   IF  "Sales Invoice Line"."VK-Preis (MW)"="Sales Invoice Line"."Unit Price" THEN
                                    BEGIN
                                     Sheet.Range('K'+j).Value := "Sales Invoice Line"."VK-Preis (MW)"*"Sales Invoice Line".Quantity;
                                    END
                                   ELSE
                                    BEGIN
                                      IF "Sales Invoice Line".Einkaufsw„hrungsfaktor <> 0 THEN
                                        Sheet.Range('K'+j).Value :=(("Sales Invoice Line"."Unit Price") / ("Sales Invoice Line".Einkaufsw„hrungsfaktor)
                                         * ("Sales Invoice Line".Quantity))
                                      ELSE
                                        Sheet.Range('K'+j).Value :=(("Sales Invoice Line"."Unit Price") * ("Sales Invoice Line".Quantity));
                                    END;
                                     Sheet.Range('L'+j).Value := FORMAT("Sales Invoice Line"."Unit Price");
                                     IF "Sales Invoice Line".Einkaufsw„hrungscode = '' THEN
                                        BEGIN
                                          Sheet.Range('M'+j).Value := 'EURO';
                                        END
                                        ELSE
                                        BEGIN
                                          Sheet.Range('M'+j).Value := "Sales Invoice Line".Einkaufsw„hrungscode ;
                                        END;
                                     Sheet.Range('N'+j).Value := "Sales Invoice Line"."Location Code";
                                     Sheet.Range('O'+j).Value := "Sales Invoice Line"."Document No.";
                                     IF ("Sales Invoice Line"."Posting Date" <> 0D)  AND ("Sales Invoice Line"."Posting Date" > 010100D)
                                        THEN Sheet.Range('P'+j).Value := "Sales Invoice Line"."Posting Date";
                                    // IF ( "Sales Invoice Line".Wunschtermin <> 0D) AND ("Sales Invoice Line".Wunschtermin > 010100D)
                                    //    THEN Sheet.Range('Q'+j).Value := "Sales Invoice Line".Wunschtermin;
                                     IF ("Sales Invoice Line"."Shipment Date" <> 0D) AND ("Sales Invoice Line"."Shipment Date" > 010100D)
                                        THEN Sheet.Range('R'+j).Value := "Sales Invoice Line"."Shipment Date";
                                        Sheet.Range('S'+j).Value := "Sales Invoice Line"."Cross-Reference No." ;
                                     email:='';
                                     ContRela.SETFILTER(ContRela."No.","Sales Invoice Line"."Sell-to Customer No.");
                                     IF ContRela.FINDFIRST THEN BEGIN
                                       Cont.SETFILTER(Cont."Company No.",ContRela."Contact No.");
                                       Cont.SETFILTER("Unternehmen verlassen",'%1',FALSE);
                                       IF Cont.FINDFIRST THEN
                                         BEGIN
                                           REPEAT
                                             pcn.RESET;
                                             pcn.SETFILTER(pcn."Contact No.",'=%1',Cont."No.");
                                             pcn.SETFILTER(pcn."Job Responsibility Code",'=%1','PCN-EOL');
                                             IF pcn.FINDFIRST THEN
                                               REPEAT
                                                 IF email = '' THEN
                                                   email := Cont."E-Mail"
                                                 ELSE
                                                   IF STRPOS(email,Cont."E-Mail") = 0 THEN
                                                     email := email + '; ' + Cont."E-Mail";
                                               UNTIL pcn.NEXT = 0;
                                               IF email <> '' THEN
                                                 Sheet.Range('T'+j).Value := email;
                                           UNTIL Cont.NEXT = 0;
                                         END;
                                       END;
