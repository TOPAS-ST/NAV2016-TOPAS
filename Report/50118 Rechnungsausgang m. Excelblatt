                                    Sheet.Range('V'+j).Value := "Sales Invoice Header"."Salesperson Code";
                                    Sheet.Range('W'+j).Value := "Sales Invoice Line"."Endkundennr.";
                                    Sheet.Range('X'+j).Value := "Sales Invoice Line".Endkunde;
                                    Sheet.Range('Y'+j).Value := "Sales Invoice Line"."Manufacturer Code";
                                    Sheet.Range('Z'+j).Value := Item."Description 2";
                                  j := INCSTR (j);
                                  END
                                  ELSE BEGIN
                                  END;
                                END;

