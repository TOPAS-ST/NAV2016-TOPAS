    LOCAL PROCEDURE OnPreSectionItem_Body3@5066(VAR Item@1000000000 : Record 27);
    BEGIN
      WITH Item DO BEGIN
        ReportForNav.ShowOutput("Sales Invoice Line".Quantity>0)
      END;
    END;

