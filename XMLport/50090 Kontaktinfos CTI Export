                    //Contact.SETFILTER(Contact."No.",'%1..%2','KT000016','KT000500');
                    //Contact.SETFILTER(Contact."No.",'%1..%2','KT000001','KT999999');
                    //Contact.SETFILTER("No.",'%1','KT023080');
                    Contact.SETFILTER("Phone No.",'<>%1','');
