    { [{1BF88755-3876-46BF-AF9A-6A9C4027C4E1}];2 ;Position            ;Element ;Field   ;
                                                  DataType=Code;
                                                  SourceField=Contact::Organizational Level Code }

    { [{DCA810F0-F6C7-4934-B48F-C8E3321C781D}];2 ;Abteilung           ;Element ;Field   ;
                                                  DataType=Text;
